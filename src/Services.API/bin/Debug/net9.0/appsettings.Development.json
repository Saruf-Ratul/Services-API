{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "JwtSettings": {
    "SecretKey": "DevelopmentSecretKeyForJWTTokenGenerationAtLeast32Characters",
    "Issuer": "ServicesAPI-Dev",
    "Audience": "ServicesAPIUsers-Dev",
    "ExpirationMinutes": 1440
  },
  "SwaggerSettings": {
    "EnableSwagger": true,
    "EnableSwaggerUI": true,
    "DocumentTitle": "Services API - Development",
    "RequireAuthentication": false,
    "RequireHttps": false
  }
}

